}



/* autocomplete */
.ui-helper-hidden-accessible {
    display: none;
}
.ui-autocomplete.ui-menu {
    .ui-menu-item {
        font-size: 12px;
    }
    .ui-autocomplete-category {
        font-size: 15px;
        line-height: 20px;
        font-style: italic;
        font-weight: bold;
    }
}

/* breadcrumbs*/
.breadcrumbs {
    li {
        color: #660000;
        display: inline-block;
        &:before {
            content: 'â€¹';
            font-size: 16px;
            padding: 0px 1px 5px 0;
        }
        &:hover {
            color: #990000;
        }
    }
}

.wrapper.admin {
    .block {
        width: 400px;
        margin: 0 40px 20px 0;
        float: left;
        border-radius: 5px;
        overflow: hidden;
        border: 1px solid #ABD4FD;
        .block_head {
            height: 30px;
            width: 100%;
            background-color: #3399ff;
            color: #FFF;
            background:linear-gradient(top, #abd4fd 0%,#3399ff 40%,#3399ff 61%,#abd4fd 100%); /* W3C */
            background:-moz-linear-gradient(top, #abd4fd 0%, #3399ff 40%, #3399ff 60%, #abd4fd 100%); /* FF3.6+ */
            background:-webkit-linear-gradient(top, #abd4fd 0%,#3399ff 40%,#3399ff 60%,#abd4fd 100%); /* Chrome10+,Safari5.1+ */
            background:-o-linear-gradient(top, #abd4fd 0%,#3399ff 40%,#3399ff 60%,#abd4fd 100%); /* Opera 11.10+ */
            h2 {
                height: 100%;
                width: 100%;
                display: inline-block;
                vertical-align: sub;
                text-align: center;
            }
        }
        .block_content {
            padding: 10px;
            table {
                tr{
                    td {
                        padding: 2px 20px 2px 0;
                    }
                }
            }
            .separator {
                width: 100%;
                height: 10px;
            }
        }
    }
    .breadcrumbs {
        margin: 0 0 7px;
    }
}

.wrapper.store {
    font-size: 16px;
